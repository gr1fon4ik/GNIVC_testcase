{
  "name": "webpack-typescript-template",
  "version": "2.1.1",
  "description": "React Webpack Typescript template",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "reduxjs/toolkit",
    "react-router",
    "eslint",
    "prettier",
    "husky"
  ],
  "author": "gr1fon4ik",
  "license": "MIT",
  "homepage": "https://github.com/gr1fon4ik/GNIVC_testcase.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gr1fon4ik/GNIVC_testcase.git"
  },
  "bugs": {
    "url": "https://github.com/gr1fon4ik/GNIVC_testcase.git"
  },
  "scripts": {
    "start": "webpack serve --progress --mode development",
    "clean": "rm -rf ./build",
    "build": "yarn clean && webpack --progress --mode production",
    "lint": "yarn eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --no-error-on-unmatched-pattern",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config .prettierrc.js --check src/**/*.{js,jsx,ts,tsx}",
    "format:fix": "prettier --config .prettierrc.js --write src/**/*.{js,jsx,ts,tsx}",
    "type": "./node_modules/.bin/tsc --project tsconfig.json --noEmit",
    "lint-staged": "lint-staged",
    "husky-install": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@svgr/webpack": "^8.0.1",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "classnames": "^2.3.2",
    "css-loader": "^7.0.0",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.57.1",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-deprecate": "^0.8.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.0",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.3",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-input-mask": "^2.0.4",
    "react-redux": "^9.1.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --no-ignore --max-warnings=0 --fix",
      "git add"
    ],
    "*.{graphql,json,scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  }
}
